// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
  // mit "@prisma/client" importieren
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Vtuber {
  id          String   @id @default(uuid())
  name        String
  avatar      String
  description String
  group       String?
  followers   Int      @default(0)
}